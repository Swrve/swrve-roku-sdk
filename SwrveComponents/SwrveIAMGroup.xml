<?xml version="1.0" encoding="utf-8" ?> 
<!-- This component provided by Swrve will automaticall display and handle IAMs -->
<component name="SwrveIAMGroup" extends="SwrveBaseGroup" >
	<interface>
        <field id="message" type="assocarray" alwaysNotify="true" onChange="OnNewMessage" />
        <field id="customActionCallback" type="string" alwaysNotify="true" />
	</interface>

	<script type="text/brightscript" uri="pkg:/components/SwrveComponents/SwrveIAMGroup.brs" />

	<children>
		<Rectangle
            id="background"
            color="0x000000"
            visible="true"
            width="1920"
            height="1080"
            opacity="0.8"
            />

	</children>
</component>
